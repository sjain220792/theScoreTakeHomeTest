<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="theScore Android Phone App zxc" configfailurepolicy="continue" verbose="10">

    <test name="Test search for Man Utd">
        <parameter name="favTeam" value="Toronto FC"/>
        <parameter name="searchTeam" value="Manchester United"/>
        <parameter name="player" value="B. Fernandes"/>
        <classes>
            <class name="core.theScore.test.TestSearchTheScoreApp"/>
        </classes>
    </test>

    <test name="Test search for TFC">
        <parameter name="favTeam" value="Toronto FC"/>
        <parameter name="searchTeam" value="Toronto FC"/>
        <parameter name="player" value="F. Bernardeschi"/>
        <classes>
            <class name="core.theScore.test.TestSearchTheScoreApp"/>
        </classes>
    </test>

</suite>